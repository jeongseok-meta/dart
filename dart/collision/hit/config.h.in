// @GENERATED_FILE_MARKER@
// Configuration file for HIT collision detection library (FCL-based)

#ifndef DART_COLLISION_HIT_CONFIG_H_
#define DART_COLLISION_HIT_CONFIG_H_

// HIT is integrated within DART - version follows DART
#define DART_COLLISION_HIT_VERSION "@DART_VERSION@"

// SSE support - disable by default for portability
// Can be enabled if needed in the future
#define DART_COLLISION_HIT_HAVE_SSE 0

// Octomap support - disable by default
// Can be enabled if needed in the future
#define DART_COLLISION_HIT_HAVE_OCTOMAP 0

// Profiling - disable by default
#define DART_COLLISION_HIT_ENABLE_PROFILING 0

// Detect the operating systems
#if defined(__APPLE__)
  #define DART_COLLISION_HIT_OS_MACOS
#elif defined(__gnu_linux__)
  #define DART_COLLISION_HIT_OS_LINUX
#elif defined(_WIN32)
  #define DART_COLLISION_HIT_OS_WINDOWS
#endif

// Detect the compiler
#if defined(__clang__)
  #define DART_COLLISION_HIT_COMPILER_CLANG
#elif defined(__GNUC__) || defined(__GNUG__)
  #define DART_COLLISION_HIT_COMPILER_GCC
#elif defined(_MSC_VER)
  #define DART_COLLISION_HIT_COMPILER_MSVC
#endif

// Deprecation macros - currently empty as HIT is internal to DART
#define DART_COLLISION_HIT_DEPRECATED
#define DART_COLLISION_HIT_DEPRECATED_EXPORT

#endif  // DART_COLLISION_HIT_CONFIG_H_
